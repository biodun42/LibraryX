@{
    ViewData["Title"] = "Privacy Policy";
}

<!-- Hero Section -->
<div class="relative bg-gradient-to-r from-primary-800 to-primary-900 overflow-hidden">
    <!-- Decorative Elements -->
    <div class="absolute inset-0 overflow-hidden opacity-20">
        <svg class="absolute top-0 left-0 transform -translate-x-1/4 -translate-y-1/4" width="404" height="384" fill="none" viewBox="0 0 404 384">
            <defs>
                <pattern id="privacy-pattern-1" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
                    <rect x="0" y="0" width="4" height="4" class="text-primary-200" fill="currentColor" />
                </pattern>
            </defs>
            <rect width="404" height="384" fill="url(#privacy-pattern-1)" />
        </svg>
        <svg class="absolute right-0 bottom-0 transform translate-x-1/3 translate-y-1/3" width="404" height="384" fill="none" viewBox="0 0 404 384">
            <defs>
                <pattern id="privacy-pattern-2" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
                    <rect x="0" y="0" width="4" height="4" class="text-primary-200" fill="currentColor" opacity="0.5" />
                </pattern>
            </defs>
            <rect width="404" height="384" fill="url(#privacy-pattern-2)" />
        </svg>
    </div>

    <div class="relative max-w-9xl mx-auto px-2 sm:px-4 lg:px-8 py-16 sm:py-24">
        <div class="text-center">
            <h1 class="text-4xl font-extrabold text-white tracking-tight sm:text-5xl lg:text-6xl">
                Privacy Policy
            </h1>
            <p class="mt-6 max-w-3xl mx-auto text-xl text-primary-100">
                At LibraryX, we respect your privacy. This policy outlines how we collect, use, and protect your information when using our library services.
            </p>
            <div class="mt-10 flex justify-center">
                <div class="inline-flex rounded-md shadow">
                    <a href="#main-content" class="inline-flex items-center justify-center px-5 py-3 border border-transparent text-base font-medium rounded-md text-primary-700 bg-white hover:bg-primary-50 transition duration-300">
                        Read the Policy
                        <svg class="ml-2 -mr-1 h-5 w-5" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7" />
                        </svg>
                    </a>
                </div>
            </div>
        </div>
    </div>
    
    <!-- Wave Divider -->
    <div class="absolute bottom-0 left-0 right-0 transform translate-y-1/2">
        <svg class="w-full text-white" viewBox="0 0 1440 100" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path fill="currentColor" d="M0,0 C240,60 480,100 720,80 C960,60 1200,20 1440,40 L1440,100 L0,100 Z"></path>
        </svg>
    </div>
</div>

<!-- Main Content -->
<div id="main-content" class="relative max-w-9xl mx-auto px-2 sm:px-4 lg:px-8 py-4 sm:py-16">
    <div class="bg-white rounded-2xl shadow-xl overflow-hidden">
        <!-- Table of Contents -->
        <div class="bg-primary-50 p-4 lg:p-10">
            <h2 class="text-2xl font-bold text-primary-800 mb-6">Table of Contents</h2>
            <div class="grid grid-cols-1 md:grid-cols-2 gap-4">
                <div class="space-y-2">
                    <a href="#information-collection" class="flex items-center text-primary-600 hover:text-primary-800 transition duration-150">
                        <span class="mr-2 text-primary-500"><i class="fas fa-circle text-xs"></i></span>
                        Membership Information
                    </a>
                    <a href="#use-of-information" class="flex items-center text-primary-600 hover:text-primary-800 transition duration-150">
                        <span class="mr-2 text-primary-500"><i class="fas fa-circle text-xs"></i></span>
                        Use of Information
                    </a>
                    <a href="#data-security" class="flex items-center text-primary-600 hover:text-primary-800 transition duration-150">
                        <span class="mr-2 text-primary-500"><i class="fas fa-circle text-xs"></i></span>
                        Data Security
                    </a>
                </div>
                <div class="space-y-2">
                    <a href="#third-party" class="flex items-center text-primary-600 hover:text-primary-800 transition duration-150">
                        <span class="mr-2 text-primary-500"><i class="fas fa-circle text-xs"></i></span>
                        Third-Party Services
                    </a>
                    <a href="#cookies" class="flex items-center text-primary-600 hover:text-primary-800 transition duration-150">
                        <span class="mr-2 text-primary-500"><i class="fas fa-circle text-xs"></i></span>
                        Cookies Policy
                    </a>
                    <a href="#your-rights" class="flex items-center text-primary-600 hover:text-primary-800 transition duration-150">
                        <span class="mr-2 text-primary-500"><i class="fas fa-circle text-xs"></i></span>
                        Your Rights
                    </a>
                </div>
            </div>
        </div>
        
        <!-- Content Sections -->
        <div class="p-2 sm:p-6 lg:p-10 space-y-12">
            <!-- Last Updated -->
            <div class="flex items-center text-sm text-gray-500 mb-8">
                <svg class="h-5 w-5 mr-2 text-primary-500" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z" />
                </svg>
                Last Updated: May 24, 2025
            </div>
            
            <!-- Introduction -->
            <div class="prose max-w-none">                
                <p class="text-lg text-gray-600 leading-relaxed">
                    This Privacy Policy describes how LibraryX ("we", "us", or "our") collects, uses, and shares your personal information when you use our library services, visit our website, borrow materials, or participate in our programs. We are committed to protecting your personal information and your right to privacy. If you have any questions or concerns about our policy or practices, please contact our library staff.
                </p>
            </div>

            <!-- Information Collection Section -->
            <div id="information-collection" class="pt-6">
                <div class="flex items-center mb-4">
                    <div class="flex-shrink-0 h-10 w-10 bg-primary-100 rounded-full flex items-center justify-center">
                        <svg class="h-6 w-6 text-primary-600" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10" />
                        </svg>
                    </div>
                    <h2 class="ml-4 text-2xl font-bold text-gray-900">Membership Information</h2>
                </div>
                <div class="ml-14 prose prose-primary max-w-none">
                    <p class="text-gray-600">
                        We collect personal information that you provide when registering for a library card, borrowing materials, using our digital resources, or participating in library programs and events.
                    </p>
                    <p class="text-gray-600">
                        The personal information we collect may include:
                    </p>
                    <ul class="mt-4 space-y-2">
                        <li class="flex items-start">
                            <span class="flex-shrink-0 h-5 w-5 text-primary-500 mr-2"><i class="fas fa-check-circle"></i></span>
                            <span>Name, address, phone number, and email address</span>
                        </li>
                        <li class="flex items-start">
                            <span class="flex-shrink-0 h-5 w-5 text-primary-500 mr-2"><i class="fas fa-check-circle"></i></span>
                            <span>Library borrowing history and currently checked-out items</span>
                        </li>
                        <li class="flex items-start">
                            <span class="flex-shrink-0 h-5 w-5 text-primary-500 mr-2"><i class="fas fa-check-circle"></i></span>
                            <span>Overdue fines and payment information</span>
                        </li>
                        <li class="flex items-start">
                            <span class="flex-shrink-0 h-5 w-5 text-primary-500 mr-2"><i class="fas fa-check-circle"></i></span>
                            <span>Records of computer and digital resource usage</span>
                        </li>
                    </ul>
                </div>
            </div>

            <!-- Use of Information Section -->
            <div id="use-of-information" class="pt-6">
                <div class="flex items-center mb-4">
                    <div class="flex-shrink-0 h-10 w-10 bg-primary-100 rounded-full flex items-center justify-center">
                        <svg class="h-6 w-6 text-primary-600" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 11.5V14m0-2.5v-6a1.5 1.5 0 113 0m-3 6a1.5 1.5 0 00-3 0v2a7.5 7.5 0 0015 0v-5a1.5 1.5 0 00-3 0m-6-3V11m0-5.5v-1a1.5 1.5 0 013 0v1m0 0V11m0-5.5a1.5 1.5 0 013 0v3m0 0V11" />
                        </svg>
                    </div>
                    <h2 class="ml-4 text-2xl font-bold text-gray-900">Use of Information</h2>
                </div>
                <div class="ml-14 prose prose-primary max-w-none">
                    <p class="text-gray-600">
                        We use the information we collect for the following library purposes:
                    </p>
                    <div class="mt-6 grid grid-cols-1 gap-6 sm:grid-cols-2">
                        <div class="bg-gray-50 p-5 rounded-lg border border-gray-100">
                            <h3 class="text-lg font-medium text-primary-700">Library Services</h3>
                            <p class="mt-2 text-sm text-gray-600">
                                To issue library cards, track borrowed materials, send due date reminders, and process reservations.
                            </p>
                        </div>
                        <div class="bg-gray-50 p-5 rounded-lg border border-gray-100">
                            <h3 class="text-lg font-medium text-primary-700">Service Improvement</h3>
                            <p class="mt-2 text-sm text-gray-600">
                                To analyze usage patterns, enhance our collection, and develop new services based on community needs.
                            </p>
                        </div>
                        <div class="bg-gray-50 p-5 rounded-lg border border-gray-100">
                            <h3 class="text-lg font-medium text-primary-700">Communications</h3>
                            <p class="mt-2 text-sm text-gray-600">
                                To notify you about holds, send event invitations, and provide library news and updates.
                            </p>
                        </div>
                        <div class="bg-gray-50 p-5 rounded-lg border border-gray-100">
                            <h3 class="text-lg font-medium text-primary-700">Collection Management</h3>
                            <p class="mt-2 text-sm text-gray-600">
                                To make informed decisions about acquiring new materials based on community interests and borrowing patterns.
                            </p>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Data Security Section -->
            <div id="data-security" class="pt-6">
                <div class="flex items-center mb-4">
                    <div class="flex-shrink-0 h-10 w-10 bg-primary-100 rounded-full flex items-center justify-center">
                        <svg class="h-6 w-6 text-primary-600" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z" />
                        </svg>
                    </div>
                    <h2 class="ml-4 text-2xl font-bold text-gray-900">Data Security</h2>
                </div>
                <div class="ml-14 prose prose-primary max-w-none">
                    <p class="text-gray-600">
                        We implement appropriate technical and organizational measures to protect your personal information. Your privacy is important to us, and we follow library best practices for data protection.
                    </p>
                    <div class="mt-6 bg-primary-50 p-4 rounded-lg border border-primary-100">
                        <h3 class="text-lg font-medium text-primary-800">Our Security Measures Include:</h3>
                        <ul class="mt-4 space-y-3">
                            <li class="flex items-start">
                                <span class="flex-shrink-0 h-5 w-5 text-primary-600 mr-2"><i class="fas fa-shield-alt"></i></span>
                                <span>Encryption of patron database and digital transactions</span>
                            </li>
                            <li class="flex items-start">
                                <span class="flex-shrink-0 h-5 w-5 text-primary-600 mr-2"><i class="fas fa-shield-alt"></i></span>
                                <span>Secure authentication for online library accounts</span>
                            </li>
                            <li class="flex items-start">
                                <span class="flex-shrink-0 h-5 w-5 text-primary-600 mr-2"><i class="fas fa-shield-alt"></i></span>
                                <span>Regular security updates to our library management system</span>
                            </li>
                            <li class="flex items-start">
                                <span class="flex-shrink-0 h-5 w-5 text-primary-600 mr-2"><i class="fas fa-shield-alt"></i></span>
                                <span>Staff training on patron privacy and confidentiality</span>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>

            <!-- Third-Party Services Section -->
            <div id="third-party" class="pt-6">
                <div class="flex items-center mb-4">
                    <div class="flex-shrink-0 h-10 w-10 bg-primary-100 rounded-full flex items-center justify-center">
                        <svg class="h-6 w-6 text-primary-600" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 12a9 9 0 01-9 9m9-9a9 9 0 00-9-9m9 9H3m9 9a9 9 0 01-9-9m9 9c1.657 0 3-4.03 3-9s-1.343-9-3-9m0 18c-1.657 0-3-4.03-3-9s1.343-9 3-9m-9 9a9 9 0 019-9" />
                        </svg>
                    </div>
                    <h2 class="ml-4 text-2xl font-bold text-gray-900">Third-Party Services</h2>
                </div>
                <div class="ml-14 prose prose-primary max-w-none">
                    <p class="text-gray-600">
                        LibraryX partners with third-party service providers to offer digital resources, e-books, online research databases, and integrated library systems. When you use these services, your information may be shared with these providers.
                    </p>
                    <p class="text-gray-600 mt-4">
                        Our third-party providers include e-book platforms, digital learning resources, and library management software. These providers have their own privacy policies, and we encourage you to review them when using these services through our library.
                    </p>
                </div>
            </div>

            <!-- Cookies Policy Section -->
            <div id="cookies" class="pt-6">
                <div class="flex items-center mb-4">
                    <div class="flex-shrink-0 h-10 w-10 bg-primary-100 rounded-full flex items-center justify-center">
                        <svg class="h-6 w-6 text-primary-600" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 3v4M3 5h4M6 17v4m-2-2h4m5-16l2.286 6.857L21 12l-5.714 2.143L13 21l-2.286-6.857L5 12l5.714-2.143L13 3z" />
                        </svg>
                    </div>
                    <h2 class="ml-4 text-2xl font-bold text-gray-900">Cookies Policy</h2>
                </div>
                <div class="ml-14 prose prose-primary max-w-none">
                    <p class="text-gray-600">
                        Our library website uses cookies to enhance your browsing experience and provide personalized services. Cookies help us remember your preferences, save your library card information for quick access, and analyze how our website is used.
                    </p>
                    <div class="mt-6 overflow-hidden bg-white border border-gray-200 rounded-lg">
                        <table class="min-w-full divide-y divide-gray-200">
                            <thead class="bg-gray-50">
                                <tr>
                                    <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Cookie Type</th>
                                    <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Library Purpose</th>
                                </tr>
                            </thead>
                            <tbody class="bg-white divide-y divide-gray-200">
                                <tr>
                                    <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">Session Cookies</td>
                                    <td class="px-6 py-4 text-sm text-gray-600">Maintain your logged-in status while navigating the catalog</td>
                                </tr>
                                <tr>
                                    <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">Preference Cookies</td>
                                    <td class="px-6 py-4 text-sm text-gray-600">Remember your search preferences and display settings</td>
                                </tr>
                                <tr>
                                    <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">Analytics Cookies</td>
                                    <td class="px-6 py-4 text-sm text-gray-600">Help us understand how patrons use our digital library resources</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

            <!-- Your Rights Section -->
            <div id="your-rights" class="pt-6">
                <div class="flex items-center mb-4">
                    <div class="flex-shrink-0 h-10 w-10 bg-primary-100 rounded-full flex items-center justify-center">
                        <svg class="h-6 w-6 text-primary-600" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z" />
                        </svg>
                    </div>
                    <h2 class="ml-4 text-2xl font-bold text-gray-900">Your Rights</h2>
                </div>
                <div class="ml-14 prose prose-primary max-w-none">
                    <p class="text-gray-600">
                        As a library patron, you have rights regarding your personal information:
                    </p>
                    
                    <div class="mt-6 space-y-6">
                        <div class="flex">
                            <div class="flex-shrink-0">
                                <div class="flex items-center justify-center h-12 w-12 rounded-md bg-primary-500 text-white">
                                    <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z" />
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M2.458 12C3.732 7.943 7.523 5 12 5c4.478 0 8.268 2.943 9.542 7-1.274 4.057-5.064 7-9.542 7-4.477 0-8.268-2.943-9.542-7z" />
                                    </svg>
                                </div>
                            </div>
                            <div class="ml-4">
                                <h3 class="text-lg font-medium text-gray-900">Right to Access</h3>
                                <p class="mt-2 text-gray-600">
                                    You can request access to your library borrowing history and account information at any time.
                                </p>
                            </div>
                        </div>

                        <div class="flex">
                            <div class="flex-shrink-0">
                                <div class="flex items-center justify-center h-12 w-12 rounded-md bg-primary-500 text-white">
                                    <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 5H6a2 2 0 00-2 2v11a2 2 0 002 2h11a2 2 0 002-2v-5m-1.414-9.414a2 2 0 112.828 2.828L11.828 15H9v-2.828l8.586-8.586z" />
                                    </svg>
                                </div>
                            </div>
                            <div class="ml-4">
                                <h3 class="text-lg font-medium text-gray-900">Right to Update</h3>
                                <p class="mt-2 text-gray-600">
                                    You can update your personal information, contact details, and notification preferences.
                                </p>
                            </div>
                        </div>

                        <div class="flex">
                            <div class="flex-shrink-0">
                                <div class="flex items-center justify-center h-12 w-12 rounded-md bg-primary-500 text-white">
                                    <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16" />
                                    </svg>
                                </div>
                            </div>
                            <div class="ml-4">
                                <h3 class="text-lg font-medium text-gray-900">Right to Privacy</h3>
                                <p class="mt-2 text-gray-600">
                                    Your borrowing history and reading preferences are confidential and protected by library privacy policies.
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Contact Us -->
            <div class="pt-10 mt-10 border-t border-gray-200">
                <h2 class="text-2xl font-bold text-center text-gray-900 mb-8">Questions About Your Library Privacy?</h2>
                <div class="flex flex-col md:flex-row bg-primary-50 rounded-xl overflow-hidden">
                    <div class="md:w-1/3 bg-primary-600 p-8 text-white">
                        <h3 class="text-xl font-semibold mb-4">Contact Us</h3>
                        <p class="mb-6">
                            If you have any questions about how we protect your privacy at LibraryX, please reach out:
                        </p>
                        <div class="space-y-4">
                            <div class="flex items-start">
                                <div class="flex-shrink-0 mt-0.5">
                                    <svg class="h-5 w-5 text-primary-200" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 8l7.89 5.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z" />
                                    </svg>
                                </div>
                                <span class="ml-3">privacy@LibraryX.org</span>
                            </div>
                            <div class="flex items-start">
                                <div class="flex-shrink-0 mt-0.5">
                                    <svg class="h-5 w-5 text-primary-200" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z" />
                                    </svg>
                                </div>
                                <span class="ml-3">(555) 123-4567</span>
                            </div>
                            <div class="flex items-start">
                                <div class="flex-shrink-0 mt-0.5">
                                    <svg class="h-5 w-5 text-primary-200" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z" />
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z" />
                                    </svg>
                                </div>
                                <span class="ml-3">123 Reading Avenue, Bookville, LX 54321</span>
                            </div>
                        </div>
                    </div>
                    <div class="md:w-2/3 p-8">
